<% @books.each do |book| %>
          <%"#{book.id}. book " %>
          <%= book.name %>
          <%= link_to 'Show', book %>
          <%= link_to 'Destroy', book, method: :delete, data: { confirm: 'Delete The Book ?' } %>
          <%= link_to 'Edit', edit_book_path(book) %>
          <%= button_to "Add", add_to_list_path(book.id)%>
          <br>

    <% end %>
<br>

<%= link_to 'New Book', new_book_path %>
